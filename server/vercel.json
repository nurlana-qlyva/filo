{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "https://filo-tau.vercel.app"
        }
    ]
}